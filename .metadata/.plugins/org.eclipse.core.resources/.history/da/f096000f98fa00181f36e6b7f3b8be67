@Repository
public class CommaDAO extends EgovAbstractDAO {

	public int selectidcheck(CommaVO vo) {
		// TODO Auto-generated method stub
		return (int) insert("commaDAO.selectidcheck", vo);
	}

	public String insertMember(CommaVO vo) {

		return (String) insert("coomaDAO.insertmember", vo);
	}
	
}
