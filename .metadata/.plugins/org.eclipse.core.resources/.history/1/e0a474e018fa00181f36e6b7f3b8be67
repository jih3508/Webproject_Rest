package egov.service.impl;

import java.util.List;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;

import egov.service.BoardVO;
import egov.service.CommaService;
import egov.service.CommaVO;
import egovframework.rte.fdl.cmmn.EgovAbstractServiceImpl;

@Service("commaService")
public class CommaServiceImpl extends EgovAbstractServiceImpl implements CommaService{
	
	@Resource(name = "commaDAO")
	private CommaDAO commaDAO;

	@Override
	public int selectidcheck(CommaVO vo) throws Exception {
		// TODO Auto-generated method stub
		return (int) commaDAO.selectidcheck(vo);
	}


	@Override
	public String insertMember(CommaVO vo) {

		return commaDAO.insertMember(vo);
	}
	

	@Override
	public String insertReBoard(BoardVO vo) throws Exception {
		return boardDAO.insertReBoard(vo);
	}





}
